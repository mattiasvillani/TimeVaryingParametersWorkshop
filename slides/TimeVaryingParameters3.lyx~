\begin_deeper
\begin_layout Itemize

\series bold
\color blue
Poisson model with time-varying intensity
\series default
\color inherit

\begin_inset Formula 
\begin{align*}
y_{t}\vert z_{t} & \sim\mathrm{Poisson}\big(\exp(z_{t})\big)\\
z_{t} & =\mu+\phi(z_{t-1}-\mu)+\eta_{t},\qquad\eta_{t}\overset{\mathrm{iid}}{\sim}N(0,\sigma_{\eta}^{2})
\end{align*}

\end_inset


\end_layout

