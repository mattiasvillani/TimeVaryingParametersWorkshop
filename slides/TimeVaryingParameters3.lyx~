\begin_deeper
\begin_layout Itemize

\series bold
\color blue
Linear Gaussian
\series default
\color inherit
 models
\begin_inset Formula 
\begin{align*}
\text{Measurement model: \hspace{0.3cm}}\boldsymbol{y}_{t} & =\boldsymbol{\boldsymbol{C}}\boldsymbol{z}_{t}+\boldsymbol{\varepsilon}_{t},\hspace*{1.15cm}\boldsymbol{\varepsilon}_{t}\overset{\mathrm{iid}}{\sim}N(\boldsymbol{0},\boldsymbol{\Sigma}_{\varepsilon})\\
\text{\text{Transition model:\hspace{1cm}}}\boldsymbol{z}_{t} & =\boldsymbol{A}\boldsymbol{z}_{t-1}+\boldsymbol{\eta}_{t},\qquad\boldsymbol{\eta}_{t}\overset{\mathrm{iid}}{\sim}N(\boldsymbol{0},\boldsymbol{\Sigma}_{\eta})
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
Non-linear
\series default
\color inherit
 (additive) 
\series bold
\color blue
Gaussian
\series default
\color inherit
 models
\begin_inset Formula 
\begin{align*}
\text{Measurement model:\hspace{0.3cm}}\boldsymbol{y}_{t} & =\boldsymbol{C}(\boldsymbol{z}_{t})+\boldsymbol{\varepsilon}_{t},\hspace*{1.15cm}\boldsymbol{\varepsilon}_{t}\overset{\mathrm{iid}}{\sim}N(\boldsymbol{0},\boldsymbol{\Sigma}_{\varepsilon})\\
\text{Transition model:\hspace{0.85cm}}\boldsymbol{z}_{t} & =\boldsymbol{A}(\boldsymbol{z}_{t-1})+\boldsymbol{\eta}_{t},\qquad\boldsymbol{\eta}_{t}\overset{\mathrm{iid}}{\sim}N(\boldsymbol{0},\boldsymbol{\Sigma}_{\eta})
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize

\series bold
\color blue
General distribution
\series default
\color inherit
 (non-linear and non-Gaussian) models
\begin_inset Formula 
\begin{align*}
\text{Measurement model:\hspace{0.3cm}}\boldsymbol{y}_{t} & \sim p(\boldsymbol{y}_{t}\vert\boldsymbol{z}_{t})\\
\text{Transition model:\hspace{0.85cm}}\boldsymbol{z}_{t} & \sim p(\boldsymbol{z}_{t}\vert\boldsymbol{z}_{t-1})
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
Hybrids:
 nonlinear measurement + linear Gaussian transition.
\end_layout

\end_deeper
